<h1>Experiments</h1>
<% @report.each do |experiment, data| %>
  <h2><%= experiment.to_s.humanize %></h2>
  <ul>
    <% data.each do |variant, metrics| %>
      <li>
        <h3><%= variant.to_s.humanize %></h3>
        <% metrics.each do |metric, count| %>
          <p><%= "#{metric.to_s.humanize} : #{count}" %></p>
        <% end %>
      </li>
    <% end %>
  </ul>
<% end %>